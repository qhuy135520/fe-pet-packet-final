{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "es6"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "allowJs": true,
    "jsx": "preserve",
    "module": "commonjs",
    "moduleResolution": "node"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
